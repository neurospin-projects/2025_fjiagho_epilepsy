   "source": [
    "def compute_distance(L,p): #p = np.array([d, e, f])\n",
    "\n",
    "    coords = np.argwhere(L == 1)  # shape (N, 3)\n",
    "\n",
    "    distances = np.linalg.norm(coords - p, axis=1)\n",
    "    min_dist = distances.min()\n",
    "\n",
    "    return min_dist\n"
   ]
